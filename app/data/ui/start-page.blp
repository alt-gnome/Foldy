using Gtk 4.0;
using Adw 1;

template $FoldyStartPage: Adw.NavigationPage {
  title: _("Start Page");
  tag: "start-page";

  child: Adw.ToolbarView {
    [top]
    Adw.HeaderBar {}

    content: Adw.StatusPage {
      icon-name: "org.altlinux.Foldy-symbolic";
      title: _("Hello!");
      description: _("Start with opening or creating a folder");
    };
  };
}
